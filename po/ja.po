msgstr "%s - %ld 分前に同期されました"

#: ../src/gtk-ui/sync-ui.c:550
#, c-format
msgid "%s - synced an hour ago"
msgstr "%s - 1 時間前に同期されました"

#: ../src/gtk-ui/sync-ui.c:554
#, c-format
